page{
  background: #f2f3f4;
}
.add{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  height: 88rpx;
  text-align: center;
  background: #4a4d5b;
  line-height: 88rpx;
  color: white;
}
navigator{
  width: 95%;
  margin: 10rpx auto;
  border-top: none;
  height: 120rpx;
  display: flex;
  background: white;
}
/* navigator:nth-child(1){
  border: 1px solid #333;
} */
navigator>view{
  flex: 1;
  display: flex;
  flex-direction: column;
  line-height: 120rpx;
}
navigator>view>view{
  flex: 1;
  padding-left: 20rpx;
  display: flex;
  line-height: 60rpx;
}
.shop-msg{
  padding-left: 20rpx;
  color:#9a9a9a;
}