.add{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  height: 88rpx;
  text-align: center;
  background: lightblue;
  line-height: 88rpx;
}
navigator{
  width: 100%;
  border: 1px solid #333;
  border-top: none;
  height: 120rpx;
  display: flex;
}
navigator:nth-child(1){
  border: 1px solid #333;
}
navigator>view{
  flex: 1;
  display: flex;
  flex-direction: column;
  line-height: 120rpx;
}
navigator>view>view{
  flex: 1;
  padding-left: 20rpx;
  display: flex;
  line-height: 60rpx;
}