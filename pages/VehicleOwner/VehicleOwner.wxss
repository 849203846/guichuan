.top-title {
  height: 50rpx;
  background: rgb(241, 241, 241);
  font-size: 12px;
  line-height: 50rpx;
  margin: 5px 0;
  padding-left: 10rpx;
  width:100%;
}
.submitbtn{
  width: 90%;
  margin: auto;
  margin-top: 20px;
  color:#fff;
  font-weight: bold;
  background: rgb(204, 68, 4);
  margin-bottom: 20px;
}
.idimgbox{
  width: 660rpx;
  margin: 20rpx auto;
}
.idimg{
  width: 320rpx;
  height: 200rpx;
  background: red;
}
.idimg:nth-child(1){
  margin-right: 20rpx;
}
.car_travel_img,.car_drive_img,.car_img{
  width:640rpx;
  height:400rpx;
  background:red;
  display: block;
  margin: 20rpx auto;
}